{
  "name": "bugradar",
  "version": "0.1.27",
  "description": "AI-powered bug tracking SDK - capture bugs visually with 3 lines of code",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "bugradar-setup": "./dist/cli/setup.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/launch-claude.d.ts",
      "import": "./dist/server/launch-claude.mjs",
      "require": "./dist/server/launch-claude.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server/launch-claude.ts src/cli/setup.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bug-tracking",
    "error-tracking",
    "debugging",
    "developer-tools",
    "ai",
    "screenshot",
    "bug-report"
  ],
  "author": "BugRadar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bugradar/bugradar-sdk"
  },
  "homepage": "https://bugradar.io",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  }
}
